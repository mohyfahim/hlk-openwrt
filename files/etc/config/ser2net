
config ser2net 'global'
	option enabled '1'

config controlport
	option port '2000'
	option host '0.0.0.0'
	option enabled '0'

config default
	option speed '115200'
	option databits '8'
	option parity 'none'
	option stopbits '1'

config proxy
	option timeout '0'
	option baudrate '115200'
	option databits '8'
	option parity 'none'
	option stopbits '1'
	option xonxoff 'false'
	option enabled '1'
	option device '/dev/ttyUSB1'
	option port '5051'
	option protocol 'raw'

